{
  "watch": [
    "src"
  ],
  "ext": ".ts",
  "ignore": [
    "node_modules",
    "data",
    "src/database/config/client",
    "src/database/queue/client"
  ],
  "exec": "node --loader tsx --require=suppress-experimental-warnings --experimental-loader=node-esm-loader --max-old-space-size=102400 --max-http-header-size=1024000 ./src/index.ts"
}