{
  "name": "nomercymediaserver",
  "productName": "NoMercy MediaServer",
  "version": "0.1.0",
  "description": "The Effortless Encoder",
  "main": "src/index.js",
  "scripts": {
    "prestart": "yarn && npx copy-files",
    "electron": "tsc && electron --unhandled-rejections=strict --trace-warnings dist/index.js",
    "rebuild": "electron-rebuild -f -w dist/index.js",
    "package": "tsc && electron-forge package",
    "make": "tsc && electron-forge make",
    "publish": "tsc && electron-forge publish",
    "pm2": "tsc && pm2 start ./dist/index.js --watch",
    "dev": "nodemon",
    "test": "jest",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "format": "eslint 'src/**/*.{js,ts}' --fix",
    "start": "yarn copy-files && node ./dist/src/index.js",
    "copy-files": "copyfiles -a ./src/database/config/**/* ./src/database/queue/**/* ./dist",
    "build": "ttsc",
    "watch": "tsc-watch --compiler ttypescript/bin/tsc --onSuccess 'yarn start'",
    "watch-only": "ttsc -w"
  },
  "keywords": [],
  "author": {
    "name": "NoMercy Entertainment",
    "email": "stoney@nomercy.tv"
  },
