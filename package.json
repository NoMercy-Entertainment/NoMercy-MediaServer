  "description": "The Effortless Encoder",
  "main": "dist/index.js",
  "scripts": {
    "prestart": "yarn",
    "electron": "tsc && electron --unhandled-rejections=strict --trace-warnings dist/index.js",
    "rebuild": "electron-rebuild -f -w  dist/index.js",
    "start": "tsc && electron-forge start",
    "package": "tsc && electron-forge package",
    "make": "tsc && electron-forge make",
    "publish": "tsc && electron-forge publish",
    "pm2": "tsc && pm2 start ./dist/index.js --watch",
    "dev": "nodemon",
    "test": "jest",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "format": "eslint 'src/**/*.{js,ts}' --fix"
  },
